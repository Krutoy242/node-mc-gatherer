{
  "packageManager": "pnpm@8.15.5",
  "name": "mc-gatherer",
  "version": "1.4.0",
  "description": "Gather recipes from different Minecraft sources",
  "main": "build/main/index.js",
  "types": "build/main/index.d.js",
  "bin": "build/main/cli.js",
  "repository": "https://github.com/Krutoy242/node-mc-gatherer",
  "license": "MIT",
  "type": "module",
  "keywords": [
    "minecraft",
    "json",
    "parser",
    "JEI",
    "crafttree"
  ],
  "scripts": {
    "build": "tsc",
    "show-unused-exports": "npx ts-prune",
    "run:e2ee": "esno src/cli.ts --mc=D:/mc_client/Instances/E2E-E --output=../src/assets/data/e2ee",
    "run:herodotus": "esno src/cli.ts --mc=D:/mc_client/Instances/Herodotus --output=../src/assets/data/herodotus"
  },
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "canvas": "^2.11.1",
    "cli-progress": "^3.12.0",
    "csv-parse": "^5.5.5",
    "glob": "^10.3.10",
    "lodash": "^4.17.21",
    "mc-icons": "^1.4.0",
    "numeral": "^2.0.6",
    "reflect-metadata": "^0.2.1",
    "terminal-kit": "^3.1.1",
    "typescript-memoize": "^1.1.1",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.9.0",
    "@types/cli-progress": "^3.11.5",
    "@types/glob": "^8.1.0",
    "@types/lodash": "^4.17.0",
    "@types/numeral": "^2.0.5",
    "@types/terminal-kit": "^2.5.6",
    "@types/yargs": "^17.0.32",
    "eslint": "^8.57.0",
    "typescript": "^5.4.3"
  },
  "files": [
    "build/main",
    "!**/*.spec.*",
    "CHANGELOG.md",
    "LICENSE",
    "README.md",
    "src/assets/scripts/*"
  ]
}
